	local function FLEDMNE_fake_script() -- Cover.LocalScript 
		local script = Instance.new('LocalScript', Cover_2)

		game:GetService("RunService").RenderStepped:Connect(function()
			script.Parent.Position = script.Parent.Parent.TopMain.Position
		end)
	end
	coroutine.wrap(FLEDMNE_fake_script)()
	local function YZEPHRU_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)
	
		local settingkeybind = true
		local mouse = game.Players.LocalPlayer:GetMouse()
		
		script.Parent.MouseButton1Down:Connect(function()
			settingkeybind = false
			script.Parent.Text = "..."
			runnin = true
		end)
		
		game:GetService("UserInputService").InputBegan:connect(function(inputObject, gameProcessedEvent)
			if inputObject.UserInputType == Enum.UserInputType.Keyboard then
				if settingkeybind == false then
					settingkeybind = true
					script.Parent.Text = inputObject.KeyCode.Name
					_G.CatLibKey =inputObject.KeyCode.Name
					if syn then
						writefile("Cat-Lib.txt", [[_G.CatLibKey = ]].. [["]].._G.CatLibKey..[["]])
					end
					runnin = false
				end
			end
		end)
		
	end
	coroutine.wrap(YZEPHRU_fake_script)()
	local function ATPW_fake_script() -- TopMain.LocalScript 
		local script = Instance.new('LocalScript', TopMain)
	
		local Cover = script.Parent.Parent.Cover
		local TopMain = script.Parent
		local shadow = script.Parent.Parent.Cover.DropShadow
		local time = 0.2 
		
		game:GetService("UserInputService").InputBegan:connect(function(inputObject, gameProcessedEvent)
			if inputObject.KeyCode.Name == _G.CatLibKey then
				if not runnin then
					runnin = true
					if TopMain.Visible == true then
						local tweena = game:GetService("TweenService"):Create(Cover, TweenInfo.new(time), {ImageTransparency = 0})
						local tween1 = game:GetService("TweenService"):Create(shadow, TweenInfo.new(time), {ImageTransparency = 0})
						tweena:Play()
						tween1:Play()
						tweena.Completed:wait()
						TopMain.Visible = false
						wait(0.5)
						local tweenb = game:GetService("TweenService"):Create(Cover, TweenInfo.new(time), {ImageTransparency = 1})
						local tween1 = game:GetService("TweenService"):Create(shadow, TweenInfo.new(time), {ImageTransparency = 1})
						tweenb:Play()
						tween1:Play()
					else
						local tweenc = game:GetService("TweenService"):Create(Cover, TweenInfo.new(time), {ImageTransparency = 0})
						local tween2 = game:GetService("TweenService"):Create(shadow, TweenInfo.new(time), {ImageTransparency = 0})
						tweenc:Play()
						tween2:Play()
						tweenc.Completed:wait()
						TopMain.Visible = true
						wait(0.5)
						local tweend = game:GetService("TweenService"):Create(Cover, TweenInfo.new(time), {ImageTransparency = 1})
						local tween3 = game:GetService("TweenService"):Create(shadow, TweenInfo.new(time), {ImageTransparency = 0})
						tweend:Play()
						tween3:Play()
					end
					runnin = false
				end
			end
		end)
	end
	coroutine.wrap(ATPW_fake_script)()
	local function ABFOOO_fake_script() -- TopMain.LocalScript 
		local script = Instance.new('LocalScript', TopMain)
	
		local UserInputService = game:GetService("UserInputService")
		
		local gui = script.Parent
		
		local dragging
		local dragInput
		local dragStart
		local startPos
		
		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
		
		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position
		
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
		
		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		
		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	coroutine.wrap(ABFOOO_fake_script)()
	local function NJPHF_fake_script() -- ScrollingFrame.LocalScript 
		local script = Instance.new('LocalScript', ScrollingFrame)
	
		local ScrollingFrame = script.Parent
		
		local ContentSizeStart = ScrollingFrame.UIGridLayout.AbsoluteContentSize.Y
		ScrollingFrame.CanvasSize = UDim2.fromOffset(0, ContentSizeStart)
		ScrollingFrame.CanvasPosition = Vector2.new(0, ContentSizeStart)
		
		if ScrollingFrame.CanvasSize.Y.Offset <= 99 and ScrollingFrame.CanvasSize.Y.Scale == 0 then
			ScrollingFrame.CanvasSize = UDim2.new(0, 0, 1, 0)
		end
		
		ScrollingFrame.ChildAdded:Connect(function()
			local ContentSize = ScrollingFrame.UIGridLayout.AbsoluteContentSize.Y
			ScrollingFrame.CanvasSize = UDim2.fromOffset(0, ContentSize)
			ScrollingFrame.CanvasPosition = Vector2.new(0, ContentSize)
		end)
	end
	coroutine.wrap(NJPHF_fake_script)()
	local function TUXUEIO_fake_script() -- Info.LocalScript 
		local script = Instance.new('LocalScript', Info)
	
		local button = script.Parent
		local AnimFrame = script.Parent.Parent.Parent.Parent.AnimFrame
		local ButtonFrame = script.Parent.Parent.Parent.Parent.Tabs[button.Name]
		button.MouseButton1Down:Connect(function()
			if ButtonFrame.Visible == false then
				local time = 0.2 
				local tweena = game:GetService("TweenService"):Create(AnimFrame, TweenInfo.new(time), {ImageTransparency = 0})
				tweena:Play()
				tweena.Completed:wait()
				ButtonFrame.Visible = true
				local tweenb = game:GetService("TweenService"):Create(AnimFrame, TweenInfo.new(time), {ImageTransparency = 1})
				tweenb:Play()
				local time = 0.2 
				local tween = game:GetService("TweenService"):Create(button, TweenInfo.new(time), {BackgroundTransparency = 0})
				tween:Play()
				for i, a in pairs(script.Parent.Parent.Parent.Parent.Tabs:GetChildren()) do
					if a.Name == button.Name then
					else
						a.Visible = false
					end
				end
				for i, b in pairs(script.Parent.Parent:GetChildren()) do
					if b.Name == button.Name or b.Name == "UIGridLayout" or b.Name == "LocalScript" then
					else
						local time = 0.2 
						local tween = game:GetService("TweenService"):Create(b, TweenInfo.new(time), {BackgroundTransparency = 1})
						tween:Play()
					end
				end
			end
		end)
	end
	coroutine.wrap(TUXUEIO_fake_script)()

