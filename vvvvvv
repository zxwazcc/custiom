				local callback = callback or function() end

				local DropdownButton = Instance.new("TextButton")
				local UICorner_7 = Instance.new("UICorner")

				DropdownButton.Name = "DropdownButton"
				DropdownButton.Parent = ScrollingFrame_3
				DropdownButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
				DropdownButton.BorderSizePixel = 0
				DropdownButton.Size = UDim2.new(0, 200, 0, 50)
				DropdownButton.ZIndex = 3
				DropdownButton.AutoButtonColor = false
				DropdownButton.Font = Enum.Font.TitilliumWeb
				DropdownButton.TextColor3 = Color3.fromRGB(83, 83, 83)
				DropdownButton.TextScaled = true
				DropdownButton.TextSize = 14.000
				DropdownButton.TextWrapped = true
				DropdownButton.Text = name

				UICorner_7.CornerRadius = UDim.new(0, 7)
				UICorner_7.Parent = DropdownButton

				local function OMIC_fake_script() -- DropdownButton.LocalScript 
					local script = Instance.new('LocalScript', DropdownButton)
				
					local Corner = script.Parent.UICorner
					local TweenService = game:GetService("TweenService")
					
					script.Parent.MouseButton1Down:Connect(function()
						local time = 0.2
						local tween = TweenService:Create(script.Parent, TweenInfo.new(time), {BackgroundColor3 = Color3.fromRGB(50, 50, 50)})
						local tween3 = TweenService:Create(Corner, TweenInfo.new(time), {CornerRadius = UDim.new(0, 10)})
						tween:Play()
						tween3:Play()
						tween.Completed:wait()
						local tween2 = TweenService:Create(script.Parent, TweenInfo.new(time), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)})
						local tween4 = TweenService:Create(Corner, TweenInfo.new(time), {CornerRadius = UDim.new(0, 7)})
						tween2:Play()
						tween4:Play()
					end)
				end
				coroutine.wrap(OMIC_fake_script)()

				DropdownButton.MouseButton1Down:Connect(function() 
					pcall(callback)
				end)
			end
			return DropLib
		end

		function ButtonLib:CreateSlider(name, minvalue, maxvalue, callback)
			local name = name or "forgor slidername ðŸ’€"
			minvalue = minvalue or 0
            maxvalue = maxvalue or 100
            callback = callback or function() end

			local mouse = game.Players.LocalPlayer:GetMouse()
            local uis = game:GetService("UserInputService")
            local Value;

			local SliderTemplate = Instance.new("Frame")
			local UICorner_9 = Instance.new("UICorner")
			local SliderName = Instance.new("TextLabel")
			local SliderMain = Instance.new("TextButton")
			local UICorner_10 = Instance.new("UICorner")
			local SliderButton = Instance.new("Frame")
			local UICorner_11 = Instance.new("UICorner")
			local SliderValue = Instance.new("TextLabel")

			SliderTemplate.Name = "SliderTemplate"
			SliderTemplate.Parent = ScrollingFrame_2
			SliderTemplate.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
			SliderTemplate.Position = UDim2.new(0.452210844, 0, 0.776289225, 0)
			SliderTemplate.Size = UDim2.new(0, 300, 0, 39)
			SliderTemplate.ZIndex = 3
			SliderTemplate.LayoutOrder = GlobalLayoutOrder + 2
			GlobalLayoutOrder = GlobalLayoutOrder + 2
			
			UICorner_9.CornerRadius = UDim.new(0, 12)
			UICorner_9.Parent = SliderTemplate
			
			SliderName.Name = "SliderName"
			SliderName.Parent = SliderTemplate
			SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderName.BackgroundTransparency = 1.000
			SliderName.Position = UDim2.new(0, 10, 0, 4)
			SliderName.Size = UDim2.new(0, 140, 0, 15)
			SliderName.ZIndex = 3
			SliderName.Font = Enum.Font.TitilliumWeb
			SliderName.TextColor3 = Color3.fromRGB(111, 111, 111)
			SliderName.TextScaled = true
			SliderName.TextSize = 14.000
			SliderName.TextWrapped = true
			SliderName.TextXAlignment = Enum.TextXAlignment.Left
			SliderName.Text = name
			
			SliderMain.Name = "SliderMain"
			SliderMain.Parent = SliderTemplate
			SliderMain.Active = false
			SliderMain.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
			SliderMain.BorderColor3 = Color3.fromRGB(35, 35, 35)
			SliderMain.Position = UDim2.new(0, 10, 0, 25)
			SliderMain.Selectable = false
			SliderMain.Size = UDim2.new(0, 280, 0, 7)
			SliderMain.ZIndex = 3
			SliderMain.AutoButtonColor = false
			SliderMain.TextTransparency = 1.000
			
			UICorner_10.CornerRadius = UDim.new(0, 12)
			UICorner_10.Parent = SliderMain
			
			SliderButton.Name = "SliderButton"
			SliderButton.Parent = SliderMain
			SliderButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
			SliderButton.BorderColor3 = Color3.fromRGB(30, 30, 30)
			SliderButton.Size = UDim2.new(0, 0, 0, 7)
			SliderButton.ZIndex = 3
			
			UICorner_11.CornerRadius = UDim.new(0, 12)
			UICorner_11.Parent = SliderButton
			
			SliderValue.Name = "SliderValue"
			SliderValue.Parent = SliderTemplate
			SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderValue.BackgroundTransparency = 1.000
			SliderValue.Position = UDim2.new(0, 150, 0, 4)
			SliderValue.Size = UDim2.new(0, 140, 0, 15)
			SliderValue.ZIndex = 3
			SliderValue.Font = Enum.Font.TitilliumWeb
			SliderValue.TextColor3 = Color3.fromRGB(70, 70, 70)
			SliderValue.TextScaled = true
			SliderValue.TextSize = 14.000
			SliderValue.TextWrapped = true
			SliderValue.TextXAlignment = Enum.TextXAlignment.Right
			SliderValue.Text = minvalue

			SliderMain.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 280) * SliderButton.AbsoluteSize.X) + tonumber(minvalue)) or 0
                pcall(function()
                    callback(Value)
                end)
                SliderButton.Size = UDim2.new(0, math.clamp(mouse.X - SliderButton.AbsolutePosition.X, 0, 280), 0, 7)
                moveconnection = mouse.Move:Connect(function()
                    SliderValue.Text = Value
                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 280) * SliderButton.AbsoluteSize.X) + tonumber(minvalue))
                    pcall(function()
                        callback(Value)
                    end)
                    SliderButton.Size = UDim2.new(0, math.clamp(mouse.X - SliderButton.AbsolutePosition.X, 0, 280), 0, 7)
                end)
                releaseconnection = uis.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 280) * SliderButton.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                        end)
                        SliderButton.Size = UDim2.new(0, math.clamp(mouse.X - SliderButton.AbsolutePosition.X, 0, 280), 0, 7)
                        moveconnection:Disconnect()
                        releaseconnection:Disconnect()
                    end
                end)
            end)
		end
		return ButtonLib
	end
	return TabLib
end
return library
