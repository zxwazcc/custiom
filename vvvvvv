			local function ODIRDY_fake_script() -- SearchBar.LocalScript 
				local script = Instance.new('LocalScript', SearchBar)
			
				local bar = script.Parent
				local buttons = script.Parent.Parent.ScrollingFrame
				
				function update()
					local search = string.lower(bar.Text)
					for i, v in pairs(buttons:GetChildren()) do
						if v:IsA("GuiButton") then
							if bar ~= "" then
								local button = string.lower(v.Text)
								if string.find(button, search) then
									v.Visible = true
								else
									v.Visible = false
								end
							else
								v.Visible = true
							end
						end
					end
				end
				
				bar.Changed:Connect(update)
			end
			coroutine.wrap(ODIRDY_fake_script)()

